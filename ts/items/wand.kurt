[def {
  Wand: {
    -img: [Comps:Rendered:image "images/tf/icons/fullcolor/icons_full_16.png" {
      x: [* 3 16]
      y: [* 30 16]
    }]

    make: (Entity:make [list
      [Comps:Located:make]
      [Comps:Rendered:make Wand:-img]
      [Comps:Portable:make]
      [Comps:Programmed:make]
      [Comps:Usable:make (user object target | object:programmed:run user target)]
    ])
  }
}]
