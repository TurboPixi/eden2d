[do

[def :Key {
  make: [| do
    [def :ent [make-ent]]
    [Loc:make ent]
    [Render:make ent "object-key"]
    [Portable:make ent]
    ent
  ]
}]

[def :Crate {
  make: [| do
    [def :ent [make-ent]]
    [Loc:make ent]
    [Render:make ent "object-crate"]
    [Portable:make ent]
    ent
  ]
}]

-- Entities with the Portable component can be moved.
[def :Portable {
  make: [ent | do
    [def ent :portable true]
  ]
}]

]
