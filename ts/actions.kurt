[def {
  action-move = [target dx dy | {
    action = :move target = target dx = dx dy = dy
    ents = [list target]
  }]

  action-use = [user target | {
    action = :use user = user target = target
    ents = [list user target]
  }]

  action-open = [opener target | {
    action = :open opener = opener target = target
    ents = [list opener target]
  }]

  action-enter = [portal ent | {
    action = :enter portal = portal ent = ent
    ents = [list portal ent]
  }]
}]
